pipeline{
agent any

tools{
maven 'maven3'

parameter
{
string (name:'TagName',defaultValue:'TC_01",description:'Cucumber tag')
string (name:'DeviceName',defaultValue:'emulator-5554',description:'Cucumber tag')
string (name:'platformVersion',defaultValue:'16',description:'Android version')
}
stages{

stage('Checkout SCM')
{

steps{
checkout scm
}
}
stage('Build')
{

steps{
bat 'mvn clean compile'
}
}
stage('Test'){
steps{
bat ***mvn test^
-Dcucucmber.filter.tags=@%TagName*^
-DdeviceName=%deviceName%^.filter.tags=@%TagName*^
-DPlatformVersion=%platformVersion%^
****

}}
stage('Deliver'){
steps{
echo 'Test Execution completed'


}}